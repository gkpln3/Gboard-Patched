<?xml version="1.0" encoding="utf-8"?>
<framework>
    <ime announce_auto_selected_candidate="true" auto_capital="false" class="com.google.android.apps.inputmethod.libs.latinguyk5.LatinIme" display_app_completions="true" language="aii-SY" string_id="ime_assyrian_syria">
        <if enable_number_row="false">
            <keyboard_group variant="assyrian" variant_label="@string/variant_assyrian">
                <keyboard def="@xml/keyboard_aii_iq" type="prime" />
                <include href="@xml/keyboards_non_prime_ascii_with_arabic_indic_digits" />
            </keyboard_group>
        </if>
        <else>
            <keyboard_group layout_theme="@style/KeyboardLayoutTheme.WithNumberRow" variant="assyrian" variant_label="@string/variant_assyrian">
                <keyboard def="@xml/keyboard_aii_iq" type="prime">
                    <merge def="@xml/keyboard_latin_fragment_numbers_10_11_11_11_no_shift" />
                </keyboard>
                <include href="@xml/keyboards_non_prime_ascii_with_arabic_indic_digits" />
            </keyboard_group>
        </else>
    </ime>
</framework>